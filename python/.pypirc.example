# Example .pypirc file for PyPI upload configuration
# Copy this to ~/.pypirc and fill in your credentials
# Or use environment variables or twine directly (recommended)

[distutils]
index-servers =
    pypi
    testpypi

[pypi]
username = __token__
password = pypi-your-api-token-here

[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
password = pypi-your-test-token-here

